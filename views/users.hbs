<div class="container">
    <div class="row">
        <h1>Users</h1>
    </div>
    
        {{#each users}}
        <br/>
        <div class="row">  
            <div class="col-md-1">
                <img src="/uploads/{{this.profilePicture}}" alt="No Image" id="profilePic" class="img-circle img-responsive" >
            </div>
            <div class="col-md-11">
                <div class="row">
                    <h3> {{this.firstName}} {{this.lastName}} </h3>
                </div>
                <div class="row">
                    <p>{{this.email}}</p>
                </div>
            </div>     
        </div>      
        {{/each}}
  
    
</div>

<script type="text/javascript" src="/javascripts/profile.js "></script>